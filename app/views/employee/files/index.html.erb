<p id="notice"><%= notice %></p>

<h1>Employee Files</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @employee_files.each do |employee_file| %>
      <tr>
        <td><%= link_to 'Show', employee_file %></td>
        <td><%= link_to 'Edit', edit_employee_file_path(employee_file) %></td>
        <td><%= link_to 'Destroy', employee_file, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Employee File', new_employee_file_path %>
