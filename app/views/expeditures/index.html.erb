<p id="notice"><%= notice %></p>

<h1>Expeditures</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @expeditures.each do |expediture| %>
      <tr>
        <td><%= link_to 'Show', expediture %></td>
        <td><%= link_to 'Edit', edit_expediture_path(expediture) %></td>
        <td><%= link_to 'Destroy', expediture, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Expediture', new_expediture_path %>
